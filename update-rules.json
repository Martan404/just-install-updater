{
  "7zip": {
    "url": "http://www.7-zip.org/download.html",
    "updater": {
      "x86":{
        "rule_type": "advanced"
      },
      "x86_64":{
        "rule_type": "advanced"
      }
    },
    "versioner": {
      "rule_type": "from-link",
      "extractor": "7z([0-9]{2})([0-9]{2}).{0,4}\\.msi$"
    }
  },
  "ccleaner": {
    "url": "https://www.piriform.com/ccleaner/download/standard",
    "updater": {
      "x86":{
        "rule_type": "css-link",
        "selector": "a.btn"
      }
    },
    "versioner": {
      "rule_type": "from-link",
      "extractor": "ccsetup([0-9]{1})([0-9]{2})\\.exe$"
    }
  },
  "clementine-player": {
    "url": "https://www.clementine-player.org/",
    "updater": {
      "x86":{
        "rule_type": "css-html-version",
        "baselink": "https://github.com/clementine-player/Clementine/releases/download/{{.version}}/ClementineSetup-{{.version}}.exe",
        "selector": "#download-version"
      }
    },
    "versioner": {
      "rule_type": "from-data",
      "extractor": "^[0-9]\\.[0-9]\\.[0-9]$"
    }
  },
  "cmake": {
    "url": "https://cmake.org/download/",
    "updater": {
      "x86":{
        "rule_type": "css-link",
        "selector": ".et_pb_text_5 > table:nth-child(6) > tbody:nth-child(1) > tr:nth-child(2) > td:nth-child(2) > a:nth-child(1)"
      }
    },
    "versioner": {
      "rule_type": "from-link",
      "extractor": "cmake-([0-9]\\.[0-9]\\.[0-9])-win.{0,6}\\.msi$"
    }
  },
  "defraggler": {
    "url": "https://www.piriform.com/defraggler/download/standard",
    "updater": {
      "x86":{
        "rule_type": "css-link",
        "selector": "a.btn"
      }
    },
    "versioner": {
      "rule_type": "from-link",
      "extractor": "dfsetup([0-9]{1})([0-9]{2})\\.exe$"
    }
  },
  "geforce-experience": {
    "url": "http://www.geforce.com/geforce-experience",
    "updater": {
      "x86":{
        "rule_type": "css-link",
        "selector": ".downloadgfe > a"
      }
    },
    "versioner": {
      "rule_type": "from-link",
      "extractor": "GeForce_Experience_v([0-9]*\\.[0-9]*\\.[0-9]*\\.[0-9]*)\\.exe$"
    }
  },
  "node": {
    "url": "https://nodejs.org/en/download/stable/",
    "updater": {
      "x86":{
        "rule_type": "css-link",
        "selector": "table.download-matrix:nth-child(4) > tbody:nth-child(1) > tr:nth-child(1) > td:nth-child(2) > a"
      },
      "x86_64":{
        "rule_type": "css-link",
        "selector": "table.download-matrix:nth-child(4) > tbody:nth-child(1) > tr:nth-child(1) > td:nth-child(3) > a"
      }
    },
    "versioner": {
      "rule_type": "from-link",
      "extractor": "node-v([0-9]*\\.[0-9]*\\.[0-9]*).{4}\\.msi$"
    }
  },
  "sourcetree": {
    "url": "https://www.sourcetreeapp.com/download/",
    "updater": {
      "x86":{
        "rule_type": "css-link",
        "selector": ".win > p:nth-child(1) > a:nth-child(1)"
      }
    },
    "versioner": {
      "rule_type": "from-link",
      "extractor": "SourceTreeSetup_([0-9]*\\.[0-9]*\\.[0-9]*)\\.exe$"
    }
  },
  "sublime-text-3": {
    "url": "https://www.sublimetext.com/3",
    "updater": {
      "x86":{
        "rule_type": "css-link",
        "selector": "#dl_win_32 > a:nth-child(1)"
      },
      "x86_64":{
        "rule_type": "css-link",
        "selector": "#dl_win_64 > a:nth-child(1)"
      }
    },
    "versioner": {
      "rule_type": "from-link",
      "extractor": "Sublime Text Build ([0-9]*)(?: x64)? Setup\\.exe$"
    }
  },
  "vlc": {
    "url": "https://www.videolan.org/vlc/download-windows.html",
    "updater": {
      "x86":{
        "rule_type": "css-link",
        "selector": "#downloadButton2"
      },
      "x86_64":{
        "rule_type": "css-link",
        "selector": "ul.dropdown-menu:nth-child(3) > li:nth-child(3) > a:nth-child(1)"
      }
    },
    "versioner": {
      "rule_type": "from-link",
      "extractor": "vlc-([0-9]*\\.[0-9]*\\.[0-9]*)-win[0-9]{2}\\.exe$"
    }
  }
}
